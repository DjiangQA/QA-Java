
public class Toohot {

	public static void main(String[] args) {
		int temp = 98;
		boolean isSummer = false;

		if (isSummer = true) {

			if (temp > 60 && temp < 100) {
				System.out.println("temp within range");
			} else {
				System.out.println("temp out of range");
			}

		}
		
		if (isSummer = false) {
			if (temp > 60 && temp < 90) {
				System.out.println("temp within range");
			} else {
				System.out.println("temp out of range");
			}

		}

	}

}
