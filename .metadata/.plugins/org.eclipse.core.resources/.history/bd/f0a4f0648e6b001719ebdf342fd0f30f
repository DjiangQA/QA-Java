
public class car extends Vehicle {
	String carName;
	String carModle;
	int carAge;
	
	public String returninfo()
	{
		return new String (carName + " " + carModle + " " + carAge + " ");
	}
	
	public car(int setID, int setPrice, String setName, String setModle, int setAge)
	{
		super (setID, setName, setPrice);
		carName = setName;
		carModle = setModle;
		carAge = setAge;
		//over load
	}
	
}
