
public class Uniquesum {

	public static void main(String[] args) 
	{
		{
			int a = 3;
			int b = 3;
			int c = 3;
			int d = 0;

			if (a == b)
			{
				a = 0;
				b = 0;
			}
			
			if (a == c)
			{
				a = 0;
				c = 0;
			}
			
			if (b == a)
			{
				b = 0;
				a = 0;
			}
			
			if (b == c)
			{
				b = 0;
				c = 0;
			}
			
			if (c == a)
			{
				c = 0;
				a = 0;
			}
			
			if (c == b)
			{
				c = 0;
				b = 0;
			}
			
			d = a+b+c;
			
			System.out.println(d);

		}

	}

}
