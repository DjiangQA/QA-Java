
public class motorcycle extends Vehicle
{
	String bikeName;
	String bikeModle;
	int bikeAge;
	int bikeCost;
	
	public String returninfo()
	{
		return new String (bikeName + " " + bikeModle + " " + bikeAge + " " + bikeCost + " ");
	}
	
	public motorcycle(int setID, String setName, int setPrice, String setModle, int setAge, int setCost)
	{
		super (setID, setName, setPrice);
		bikeName = setName;
		bikeModle = setModle;
		bikeAge = setAge;
		bikeCost  = setCost;
		//over load
	}
}
