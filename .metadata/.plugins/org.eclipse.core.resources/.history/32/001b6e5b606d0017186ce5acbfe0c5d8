package main;

public class Main {

	public static void main(String[] args) 
	{
		spiralRoot(5);
	}
	
	public static void spiralRoot (int n)
	{
		int result = 0;
		int maxnum = n*n;
		while(Math.sqrt(result) <= n)
			{
				//System.out.println(result);
				result++;
			}
		
		for (int i = 1; i <= n*n ; i ++)
		{
			System.out.println(i);
		}
	}

}
