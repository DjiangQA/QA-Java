
public class Helloworld {
	
	public static void main(String[] args) 
	{
		int[] arrays = new int[10];
		int[] arrays2 = new int[10];
		int[] arrays3 = new int[10];
		int master = 0;
		
		String text = "Hellow World!"; // Q1
		System.out.println(text);
		System.out.println();
	    
		Q3("Hellow World! 2"); // Q2
	    System.out.println();
	    
	    System.out.println(Q4()); // Q3
	    System.out.println();
	    
	    System.out.println(Q5(1,2)); // Q4
	    System.out.println();
	    
	    System.out.println(Q6(2,3, false)); // Q5
	    System.out.println();
	    
	    System.out.println(Q7(1,0)); // Q6
	    System.out.println();
	    
	    for (int i = 0 ; i<10 ; i++) // Q7
	    {
	    	//System.out.println(); 
	    	System.out.println(Q7(1,i));
	    	int temp = 1 + i;
	    	arrays[i] = temp;
	    }
	    
	    System.out.println();
	    
	    for (int i2 = 0; i2 < 10 ; i2++) // Q8/9/10
	    {
	    	System.out.println(arrays[i2]);
	    }
	    
	    System.out.println();
	    for (int i3 = 0; i3 < 10 ; i3++) // Q11
	    {
	    	int temp1 = i3+1;
	    	arrays2[i3] = temp1;
	    	System.out.println(arrays[i3]);
	    }
	    
	    for (int i4 = 0; i4 < 10 ; i4++)
	    {
	    	int temp2 = arrays2[i4] * 10;
	    	
	    	System.out.println(temp2);
	    	
	    }
	    
	}
	
	public static void Q3 (String text1)
	{
		System.out.println(text1);
	}
	
	public static String Q4()
	{
		return("Hellow World! 3");
	}
	
	public static int Q5 (int a, int b)
	{
		return(a+b);
	}
	
	public static int Q6 (int a, int b, boolean c)
	{
		if (c == true)
		{
			return (a+b);
		}
		else
		{
			return (a*b);
		}
	}
	
	public static int Q7 (int a, int b)
	{
		return (a+b);
	}
	
	
	
	
	
}