
public class car extends Vehicle {
	String carName;
	String carModle;
	boolean allWheelDrive;
	
	public String returninfo()
	{
		return new String (carName + " " + carModle + " " + allWheelDrive + " ");
	}
	
	public car(int setID, String setName, int setPrice, String setModle, boolean setDrive)
	{
		super (setID, setName, setPrice);
		this.vehicleID = setID;
		this.carName = setName;
		this.carModle = setModle;
		this.allWheelDrive = setDrive;
		//over load
	}
	
}
