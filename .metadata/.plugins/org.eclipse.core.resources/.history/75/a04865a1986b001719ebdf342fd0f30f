
public class car extends Vehicle {
	String carName;
	String carModle;
	boolean allWheelDrive;
	
	public String returninfo()
	{
		return new String (carName + " " + carModle + " " + allWheelDrive + " ");
	}
	
	public car(int setID, String carName, int setPrice, String carModle, boolean allWheelDrive)
	{
		super (setID, carName, setPrice);
		this.carName = carName;
		this.carModle = carModle;
		this.allWheelDrive = allWheelDrive;
		//over load
	}
	
}
