
public class Vehicle {
	int vehicleID;
	String vehicleName;
	int Vehicleprice;
	
	public static void vehicles(String[] args) {
		car BMW = new car(1,20000,"BMW","M3",24);
		
		System.out.println(BMW.returninfo());
	}
	
	public Vehicle(int setID, String setName, int setPrice)
	{
		vehicleID = setID;
		vehicleName = setName;
		Vehicleprice = setPrice;
		//over load
	}
	
}
